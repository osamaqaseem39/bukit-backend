            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": ["{{base_url}}"],
              "path": ["auth", "login"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "let json = {};\n",
                  "try {\n",
                  "  json = pm.response.json();\n",
                  "} catch (e) {}\n",
                  "if (json && json.access_token) {\n",
                  "  pm.collectionVariables.set(\"token\", json.access_token);\n",
                  "}\n",
                  "pm.test(\"Login succeeded\", function () {\n",
                  "  pm.expect(pm.response.code).to.be.oneOf([200, 201]);\n",
                  "});"
                ]
              }
            }
          ]
        },
